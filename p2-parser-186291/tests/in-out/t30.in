class Main {
    public int f(int n, int k) {
        int p, q, t;
        if (n < 2) {
            k = 0;
            return n;
        }
        else {
            t = f(n-1, p) + f(n-2, q);
            k = p + q + 1;
            return t;
        }
    }

    public static void main(String[] args) {
        Main obj = new Main();
        int m = 9;
        print(obj.f(3, m), m);
    }
}