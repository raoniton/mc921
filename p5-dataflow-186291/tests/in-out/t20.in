// add and subtract
class Program {
    public int add(int x, int y) {
        return x + y;
    }

    public int subtract(int x, int y) {
        return x - y;
    }

    public static void main(String[] args) {
        int  foo = 1, bar = 1;
        print(foo, " + ", bar, " = ", this.add(foo, bar), "  ");
        print(foo, " - ", bar, " = ", this.subtract(foo, bar));
    }
}