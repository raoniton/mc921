class Main {
    public static void main(String[] args) {
        int n=1001, r = 0, t;

        t = n;

        while (t != 0)
        {
            r = r * 10;
            r = r + t%10;
            t = t/10;
        }

        if (n == r)
            print(n, " is a palindrome number.");
        else
            print(n, " isn't a palindrome number.");
    }
}