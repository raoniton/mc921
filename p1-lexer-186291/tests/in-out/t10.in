class Main {
    public int f(int m, int n) {
        if (!(n < 2)) {
            n = n * 2;
            return n;
        } else {
            m = m % 2;
            return m;
        }
    }

    public static void main(String[] args) {
        Main obj = new Main();
        int m = 9;
        int n = 4;
        print(obj.f(m, n));
    }
}