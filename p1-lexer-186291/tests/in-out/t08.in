class Main {
    public static void main(String[] args) {
        int n, r, sum = 0, temp;
        n = 1001;
        temp = n;
        while (n > 0) {
            r = n % 10;
            sum = (sum * 10) + r;
            n = n / 10;
        }
        if (temp == sum)
            print("palindrome number");
        else
            print("not palindrome");
    }
}